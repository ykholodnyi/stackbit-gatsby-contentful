        "@types/react": "^18.0.28",
        "@types/react-dom": "^18.0.11",
        "autoprefixer": "^10.4.14",
        "contentful-export": "^7.19.44",
        "contentful-import": "^8.5.42",
        "gatsby-plugin-postcss": "^6.8.0",
